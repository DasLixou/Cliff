<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="350" d:DesignHeight="600"
             Width="350"
             x:Class="Cliff.Design.Components.LibraryWidget">
  <Grid>
    <Rectangle Fill="{DynamicResource Stone/800}" IsHitTestVisible="False" />
    <ItemsPresenter VerticalAlignment="Top" Items="{Binding LibraryEntries}" />
  </Grid>
</UserControl>
